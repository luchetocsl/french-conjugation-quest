{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": false,
			"dynamicImport": true
		},
		"transform": {
			"react": {
				"runtime": "automatic",
				"throwIfNamespace": false,
				"development": false,
				"useBuiltins": false
			}
		},
		"target": "es2022"
	},
	"module": {
		"type": "es6"
	}
}
